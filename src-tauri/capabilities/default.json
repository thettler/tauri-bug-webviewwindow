{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "webview:default",
    "window:default",
    "window:allow-create",
    "webview:allow-create-webview",
    "webview:allow-create-webview-window",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default"
  ]
}
